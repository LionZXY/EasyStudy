<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PressedButtonAttrs">
        <attr name="normalColor" format="color" />
        <attr name="onPressedColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RelativeLayoutWithLine">
        <attr name="colorLine" format="color" />
        <attr name="paddingRight" format="dimension" />
        <attr name="paddingLeft" format="dimension" />
        <attr name="paddingTop" format="dimension" />
        <attr name="paddingBottom" format="dimension" />
        <attr name="sizeLine" format="dimension"/>
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="gravity" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
    </declare-styleable>
</resources>